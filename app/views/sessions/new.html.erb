<h1>Please login to continue</h1>


  <%= form_tag sessions_path do %>
<div class = 'log_in'>
    <div class="field">
      <%= label_tag :email %> <br>
      <%= text_field_tag :email, params[:email] %>
    </div>
    </div>
   <div class = 'log_in'> 
    <div class="field">
      <%= label_tag :password  %> <br>
      <%= password_field_tag :password %>
    </div>
   </div> 
    <BR>
    <div class="actions"><%= submit_tag "Log in" %></div>
  <% end %>
  </div>